{
  "name": "theoj-web",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "generate-openapi": "cargo run --bin --no-default-features --bin gen_openapi ./openapi.json && npx openapi-typescript-codegen -i ./openapi.json -o crates/theoj-web/src/theoj-api",
    "watch-theoj-api": "watchexec -r -w \"crates/theoj-api/\" -w \"crates/theoj-common/\" \"cargo run --no-default-features --bin theoj-api\"",
    "watch-theoj-judge": "watchexec -r -w \"crates/theoj-judge/\" -w \"crates/theoj-common/\" \"systemd-run --user --scope -p Delegate=yes -- cargo run --bin theoj-judge -- -c judge_config.yml serve\"",
    "dev": "vite crates/theoj-web",
    "build": "vue-tsc -b && vite build crates/theoj-web",
    "preview": "vite preview crates/theoj-web"
  },
  "dependencies": {
    "@iconify-json/heroicons": "^1.2.3",
    "@tailwindcss/typography": "^0.5.19",
    "@vscode/markdown-it-katex": "^1.1.2",
    "jszip": "^3.10.1",
    "katex": "^0.16.25",
    "markdown-it": "^14.1.0",
    "pinia": "^3.0.3",
    "theme-change": "^2.5.0",
    "vue": "^3.5.22",
    "vue-router": "4",
    "vue-toastification": "^2.0.0-rc.5"
  },
  "devDependencies": {
    "@biomejs/biome": "2.2.5",
    "@iconify/vue": "^5.0.0",
    "@types/markdown-it": "^14.1.2",
    "@types/node": "^24.6.0",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vue/tsconfig": "^0.8.1",
    "autoprefixer": "^10.4.21",
    "daisyui": "4",
    "openapi-typescript-codegen": "^0.29.0",
    "postcss": "^8.5.6",
    "tailwindcss": "3",
    "typescript": "~5.9.3",
    "vite": "npm:rolldown-vite@latest",
    "vue-tsc": "^3.1.0"
  }
}